  <subProcess description="" id="_-zgv4KLZEd6qz86y5ipz-A" generationCondition="md:paramValue($this,'loadData')='true' or md:paramValue($this,'loadData')='synchronize'" isBeginAction="false" name="Load data" nbCycles="-1" repetitionQuery="" toleratedError="false">
    <link id="_6MFdINZGEeaftYZqRINXbA" executionType="OK" generationType="OK_KO" mandatory="1" targetId="_z0-KENZGEeaftYZqRINXbA?fileId=_zZDewaLZEd6qz86y5ipz-A$type=proc$name=Synchronize%20data?"/>
