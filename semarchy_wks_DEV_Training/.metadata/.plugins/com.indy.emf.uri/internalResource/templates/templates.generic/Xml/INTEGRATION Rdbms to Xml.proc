&lt;!--		&lt;xsl:value-of select=&quot;$REF/tech:groupByPart(current())&quot;/>-->&#xD;
&lt;!-- calculÃ© automatiquement-->&#xD;
		&lt;/where>&#xD;
		&lt;/query>&#xD;
		&#xD;
		&lt;/sourceSet>&#xD;
	&lt;/xsl:for-each>&#xD;
&lt;/xsl:variable>&#xD;
&amp;lt;data targetNamespace=&quot;%x{$REF/ref:target()/../@targetNamespace}x%&quot;>&#xD;
