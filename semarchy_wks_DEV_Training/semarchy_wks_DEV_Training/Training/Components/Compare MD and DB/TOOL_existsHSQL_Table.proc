  <actionCode id="782f865d-f7a0-47af-96d6-d1c440f0d15e" isBeginAction="false" name="Bean Scripting" nbCycles="-1" toleratedError="false" technology="com.indy.engine.actionCodes.BeanScriptingActionCodeI">
