    <parameter id="_7_LiwTbiEeaJGbsP6B4HOA" name="SQL_CONNECTION" type="String"/>
    <code>INSERT INTO PUBLIC.GRANT_DETAILS VALUES ('FIL',&#xD;
                                        ':{Instance_name}:',&#xD;
                                        ':{Schema_name}:',&#xD;
                                        '%x{md:paramValue($this,'GranteeSchemaName')}x%',&#xD;
